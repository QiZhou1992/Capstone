<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="data.view.ClassColumnController">
   <children>
      <Label layoutX="72.0" layoutY="43.0" text="Column" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="20.0" />
      <GridPane fx:id="gridPane" prefHeight="123.0" prefWidth="520.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="50.0">
         <children>
            <Label text="Title" />
            <Label text="Description" GridPane.rowIndex="1" />
            <TextField fx:id="title" promptText="enter title here..." GridPane.columnIndex="1" />
            <TextField fx:id="description" promptText="enter some description..." GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Represent" GridPane.rowIndex="2" />
            <ComboBox fx:id="represent" prefHeight="34.0" prefWidth="211.0" promptText="Select Represent" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Button fx:id="apply" layoutX="464.0" layoutY="518.0" mnemonicParsing="false" onAction="#handleApply" text="Apply" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="50.0" />
   </children>
</AnchorPane>
